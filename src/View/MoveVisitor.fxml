<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ComboBox?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="600.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="View.MoveVIsitorController">
   <children>
      <Label layoutX="115.0" layoutY="33.0" text="Move Visitor to a new Section">
         <font>
            <Font name="System Bold" size="25.0" />
         </font>
      </Label>
      <Label layoutX="81.0" layoutY="164.0" prefHeight="27.0" prefWidth="129.0" text="Select a visitor:">
         <font>
            <Font size="18.0" />
         </font>
      </Label>
      <Label layoutX="81.0" layoutY="272.0" prefHeight="27.0" prefWidth="152.0" text="Select the Section:">
         <font>
            <Font size="18.0" />
         </font>
      </Label>
      <ComboBox fx:id="visitorList" layoutX="253.0" layoutY="165.0" prefHeight="25.0" prefWidth="182.0" />
      <ComboBox fx:id="sectionList" layoutX="253.0" layoutY="273.0" prefHeight="25.0" prefWidth="182.0" />
      <Button layoutX="233.0" layoutY="379.0" mnemonicParsing="false" onAction="#move" text="Move Visitor " />
      <Button fx:id="backbtn" layoutX="14.0" layoutY="38.0" mnemonicParsing="false" onAction="#back" prefHeight="25.0" prefWidth="66.0" />
   </children>
</AnchorPane>
